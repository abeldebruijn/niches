# This environment variable is used in Production and Preview environments to deploy your Convex project
# This variable is not needed in development.
# You can create a Production or Preview deploy key at https://dashboard.convex.dev/project/settings#production-deploy-keys
CONVEX_DEPLOY_KEY=

# Google Cast custom receiver app ID (required for Chromecast sender controls)
NEXT_PUBLIC_CAST_APP_ID=ThisIsAPlaceholderAppIdReplaceMeWithYourOwn

# Optional namespace override used for sender -> receiver lobby updates
NEXT_PUBLIC_CAST_NAMESPACE=urn:x-cast:com.niches.lobby

